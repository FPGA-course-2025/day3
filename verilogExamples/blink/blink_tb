#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
S_0x55e114912600 .scope module, "blinkModule_tb" "blinkModule_tb" 2 1;
 .timescale 0 0;
v0x55e1149230a0_0 .var "clock", 0 0;
v0x55e114923140_0 .net "out", 0 0, L_0x55e114923210;  1 drivers
S_0x55e114912790 .scope module, "blinkGate" "blinkModule" 2 20, 3 1 0, S_0x55e114912600;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "A";
    .port_info 1 /OUTPUT 1 "B";
L_0x55e114923210 .functor BUFZ 1, v0x55e1149230a0_0, C4<0>, C4<0>, C4<0>;
v0x55e114914350_0 .net "A", 0 0, v0x55e1149230a0_0;  1 drivers
v0x55e114922f80_0 .net "B", 0 0, L_0x55e114923210;  alias, 1 drivers
    .scope S_0x55e114912600;
T_0 ;
    %delay 1, 0;
    %load/vec4 v0x55e1149230a0_0;
    %nor/r;
    %store/vec4 v0x55e1149230a0_0, 0, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55e114912600;
T_1 ;
    %vpi_call 2 10 "$dumpfile", "blink.vcd" {0 0 0};
    %vpi_call 2 11 "$dumpvars" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x55e114912600;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e1149230a0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "blink_tb.v";
    "blink.v";
